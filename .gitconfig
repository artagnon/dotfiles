[include]
	path = ~/private/.gitconfig
[core]
	whitespace = trailing-space,space-before-tab
[apply]
	whitespace = fix
[user]
	name = Ramkumar Ramachandra
	email = artagnon@gmail.com
[github]
	user = artagnon
[format]
	signoff = true
	coverLetter = auto
[sendemail]
	smtpuser = artagnon@gmail.com
	suppresscc = all
	smtpserver = smtp.gmail.com
	smtpserverport = 587
	smtpencryption = tls
	chainreplyto = false
[color]
	ui = auto
[push]
	default = current
[pretty]
	lg = format:%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr %an)%Creset
[alias]
	l = log
	lf = log --follow
	sl = shortlog -ns --no-merges
	s = status -sb
	sh = show
	c = clone
	co = checkout
	cob = checkout -b
	ci = commit
	cp = cherry-pick
	fp = !rm -rf outgoing && git format-patch -M -C -o outgoing
	fpo = format-patch --stdout -M -C
	se = send-email
	graph = log --graph --decorate --pretty=oneline --abbrev-commit
	rv = log -p --reverse origin..
	r = reset
	rh = reset --hard
	re = rebase
	ri = rebase --interactive
	rc = rebase --continue
	ra = rebase --abort
	rs = rebase --skip
	ss = stash save
	sp = stash pop
	sd = stash drop
	sc = stash clear
	p = push
	pf = push -f
	f = fetch
	fa = fetch --all
	pu = pull
	b = blame -M -CCC
	bD = branch -D
	bM = branch -M
	br = branch
	hot = for-each-ref --pretty='%C(red)%(HEAD)%C(reset) %C(green)%(refname:short)%C(reset)%(upstream:trackshort)' --count 10 --sort='-committerdate' refs/heads
	bra = branch -a
	test-ri = !GIT_EDITOR='sed -i /^pick/agit-prove' git ri
	m = merge
	fb = filter-branch
	rp = rev-parse
	rems = remote show -n
	rema = remote add
	remr = remote rm
	remp = remote prune
	remn = rename rename
	su = remote set-url
	d = diff
	g = grep
	rl = log --oneline --relative-date -g
[pull]
	rebase = true
[merge]
	conflictstyle = diff3
	log = 10
	ff = only
	defaultToUpstream = true
[rerere]
	enabled = true
[rebase]
	autosquash = true
	autostash = true
[diff]
	submodule = log
[grep]
	lineNumber = true
[advice]
	pushUpdateRejected = false
	statusHints = false
	commitBeforeMerge = false
	resolveConflict = false
	implicitIdentity = false
	detachedHead = false
	amWorkDir = false
[credential]
	helper = cache --timeout 36000
[url "git@github.com:artagnon/"]
	insteadOf = gh:artagnon/
[url "git@github.com:klug/"]
	insteadOf = gh:klug/
[url "git://github.com/"]
	insteadOf = gh:
