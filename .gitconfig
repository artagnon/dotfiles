[include]
	path = ~/private/.gitconfig
[core]
	whitespace = trailing-space,space-before-tab
[apply]
	whitespace = fix
[user]
	name = Ramkumar Ramachandra
	email = artagnon@gmail.com
[github]
	user = artagnon
[format]
	signoff = true
	coverLetter = auto
[sendemail]
	smtpuser = artagnon@gmail.com
	suppresscc = self
	smtpserver = smtp.gmail.com
	smtpserverport = 587
	smtpencryption = tls
	chainreplyto = false
[push]
	default = current
[pretty]
	lg = %C(red)%h%C(reset) -%C(yellow)%d%C(reset) %s %C(green)(%cr %an)%C(reset)
[alias]
	l = log
	lr = log --reverse --no-merges
	lp = log -p --no-merges
	lrp = log --reverse -p --no-merges
	lf = log --follow
	sl = shortlog -ns --no-merges
	sle = shortlog -nse --no-merges
	sla = !git shortlog -ns --no-merges --author=\"$(git config --get user.name)\"
	s = status
	sh = show
	shs = show -s
	c = clone
	co = checkout
	cob = checkout -b
	ci = commit
	cia = commit --amend
	cp = cherry-pick
	cpc = cherry-pick --continue
	cpa = cherry-pick --abort
	cpq = cherry-pick --quit
	fp = !rm -rf outgoing && git format-patch -M -C -o outgoing
	fp1 = !rm -rf outgoing && git format-patch -1 -M -C -o outgoing
	fpm = !rm -rf outgoing && git format-patch -M -C -o outgoing master
	fpo = format-patch --stdout -M -C
	se = send-email
	graph = log --graph --decorate --pretty=oneline --abbrev-commit
	rv = log --reverse origin..
	rvp = log -p --reverse origin..
	r = reset
	rh = reset --hard
	rhu = reset --hard @{u}
	re = rebase
	ri = rebase -i
	rim = rebase -i master
	rc = rebase --continue
	ra = rebase --abort
	rs = rebase --skip
	ss = stash save
	sp = stash pop
	sd = stash drop
	sc = stash clear
	p = push
	pup = push -u
	pf = push -f
	f = fetch
	fo = fetch origin
	fa = fetch --all
	pu = pull
	b = blame -M -CC
	bD = branch -D
	bM = branch -M
	br = branch
	bru = branch -u
	fer = for-each-ref
	hot = for-each-ref --format='%(color:red)%(HEAD) %(color:green)%(refname:short)%(color:reset)%(upstream:trackshort)' --count 10 --sort='-committerdate' refs/heads
	bra = branch -a
	m = merge
	fb = filter-branch
	rp = rev-parse
	rl = rev-list
	rsh = remote show -n
	rad = remote add
	rrm = remote rm
	rpr = remote prune
	rre = remote rename
	rsu = remote set-url
	sad = submodule add
	d = diff
	g = grep -p
	fg = !git ls-files | grep -E
	rfl = log --oneline --relative-date -g
	conf = config
	desc = describe --always --contains --all
	ref = log -1 --date=short --pretty='%h (%s, %ad)'
	h = help
	cf = cat-file
[man]
	viewer = eman
[status]
	short = true
	branch = true
[pull]
	rebase = true
[merge]
	conflictstyle = diff3
	log = 10
	defaultToUpstream = true
[rerere]
	enabled = true
[rebase]
	autosquash = true
	autostash = true
[diff]
	submodule = log
	tool = vimdiff2
[pack]
	usebitmaps = true
[grep]
	lineNumber = true
[log]
	date = relative
	mailmap = true
[blame]
	date = relative
	blankboundary = true
	showroot = true
[advice]
	pushUpdateRejected = false
	statusHints = false
	commitBeforeMerge = false
	resolveConflict = false
	implicitIdentity = false
	detachedHead = false
	amWorkDir = false
[clean]
	requireForce = false
[credential]
	helper = cache --timeout 36000
[url "git@github.com:artagnon/"]
	insteadOf = gh:artagnon/
[url "git@github.com:klug/"]
	insteadOf = gh:klug/
[url "git://github.com/"]
	insteadOf = gh:
